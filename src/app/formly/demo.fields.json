[
	{
		"key": "fullName",
		"type": "input",
		"templateOptions": {
			"label": "Full Name"
		}
	},
	{
		"key": "email",
		"type": "input",
		"templateOptions": {
			"label": "Email address",
			"placeholder": "Enter email",
			"required": true
		}
	},
	{
		"key": "numberOfKeyboards",
		"type": "input",
		"templateOptions": {
			"label": "Number of Keyboards",
			"type": "number",
			"max": 10
		},
		"modelOptions": {
			"updateOn": "change"
		}
	},
	{
		"key": "ipAddress",
		"type": "input",
		"templateOptions": {
			"label": "IP Address"
		},
		"validators": {
			"validation": ["ip"]
		}
	},
	{
		"template": "<br/><hr/><br/>"
	},
	{
		"key": "colorTypeId",
		"type": "select",
		"templateOptions": {
			"label": "Color Type",
			"options": []
		},
		"expressionProperties": {
			"templateOptions.options": "formState.selectOptionsData.colorTypes"
		}
	},
	{
		"key": "favoriteColorId",
		"type": "select",
		"templateOptions": {
			"label": "Favorite Color",
			"options": []
		},
		"expressionProperties": {
			"hide": "!model.colorTypeId && model.colorTypeId !== 0",
			"model.favoriteColorId": "(!model.colorTypeId && model.colorTypeId !== 0) ? null : model.favoriteColorId",
			"templateOptions.options": "formState.selectOptionsData.colors"
		}
	},
	{
		"template": "<br/><hr/><br/>"
	},
	{
		"key": "likesMusic",
		"type": "radio",
		"templateOptions": {
			"label": "Likes Music?",
			"options": [
				{ "value": true, "label": "Yes" },
				{ "value": false, "label": "No" }
			]
		}
	},
	{
		"key": "favoriteGenreId",
		"type": "autocomplete",
		"templateOptions": {
			"label": "Favorite Music",
			"options": [],
			"valueProp": "id",
			"labelProp": "name"
		},
		"hideExpression": "!model.likesMusic",
		"expressionProperties": {
			"templateOptions.options": "formState.selectOptionsData.genres",
			"model.favoriteGenreId": "!model.likesMusic ? null : model.favoriteGenreId"
		}
	},
	{
		"template": "<br/><hr/><br/>"
	},
	{
		"key": "address",
		"wrappers": ["panel"],
		"templateOptions": { "label": "Address" },
		"fieldGroup": [
			{
				"key": "street",
				"type": "input",
				"templateOptions": {
					"label": "Street"
				}
			},
			{
				"key": "city",
				"type": "input",
				"templateOptions": {
					"label": "City"
				}
			}
		]
	}
]
